class sum{
	public static void main (String [] args){
		int count1=0,count2=0;
		int[] arr = {10, 20, 30, 40, 55};
		for (int i =0;i<arr.length ;i++ ){
			if (arr[i]%2==0)
			{
			  count1++;
			}
			else{count2++;}
			
		}
		System.out.print(count1);
		System.out.print(count2);

			
		}
}