class test2 {
	public static void main (String [] args){
		int [] arr = {2,1,5,1,3,2};
		int k = 7,sum=0,len=0,max=0,left=0;
		
		for (int i =0;i<=arr.length;i++ ){
			sum+= arr[i];
		while (sum>k){
			sum-=arr[left];
			left++;
			}
			if (sum==k){
				len = i-left+1;
			}
			if (len>max){
				max = len;}
		}
		
		System.out.println(max);}
}
			